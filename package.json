{
  "name": "traitdunion",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install": "run-p install:front install:api",
    "install:front": "cd front && npm install",
    "install:api": "cd backend && npm install",
    "build": "run-p build:front build:api",
    "build:front": "cd front && npm run build",
    "build:api": "cd backend && npm run build",
    "test": "run-p test:front test:api",
    "test:front": "cd front && npm run test",
    "test:api": "cd backend && npm run test",
    "lint": "run-p lint:front lint:api",
    "lint:front": "cd front && npm run lint",
    "lint:api": "cd backend && npm run lint",
    "start": "run-p start:front start:api",
    "start:front": "cd front && npm run start",
    "start:api": "cd backend && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/trait-d-union.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/betagouv/trait-d-union/issues"
  },
  "homepage": "https://github.com/betagouv/trait-d-union#readme",
  "dependencies": {
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "typescript": "3.4.2"
  }
}
