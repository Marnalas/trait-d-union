{
  "name": "traitdunion",
  "version": "0.0.157",
  "description": "Trait d'Union",
  "scripts": {
    "postinstall": "run-p --print-label install:front install:api",
    "install:front": "cd front && npm install",
    "install:api": "cd api && npm install",
    "build": "run-s --print-label build:front cp:front",
    "build:front": "cd front && npm run build",
    "cp:front": "mkdir -p dist/candidat && cp -r front/dist/* dist/candidat",
    "test": "run-p --print-label test:front test:api test:e2e:api",
    "test:front": "cd front && npm run test",
    "test:api": "cd api && npm run test",
    "test:api:watch": "cd api && npm run test:watch",
    "lint": "run-p --print-label lint:front lint:api",
    "lint:fix": "run-p --print-label lint:front:fix lint:api:fix",
    "lint:front": "cd front && npm run lint",
    "lint:front:fix": "cd front && npm run lint -- --fix",
    "lint:api": "cd api && npm run lint",
    "lint:api:fix": "cd api && npm run lint -- --fix",
    "start": "run-p --print-label start:api start:front",
    "start:front": "cd front && npm run start",
    "start:api": "cd api && npm run start",
    "start:dev": "run-p --print-label start:dev:api start:dev:front",
    "start:dev:api": "cd api && npm run start:dev",
    "start:dev:front": "cd front && npm run start:dev",
    "test:e2e:api": "cd api && npm run test:e2e",
    "preversion": "run-p --print-label preversion:api preversion:front",
    "preversion:api": "cd api && npm --no-git-tag-version version patch && git add .",
    "preversion:front": "cd front && npm --no-git-tag-version version patch && git add ."
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/trait-d-union.git"
  },
  "author": "Beta Gouv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/trait-d-union/issues"
  },
  "homepage": "https://github.com/betagouv/trait-d-union#readme",
  "dependencies": {
    "ember-cli": "3.11.0",
    "husky": "2.4.1",
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "cucumber": "5.1.0",
    "cucumber-pretty": "1.5.2",
    "dirty-chai": "2.0.1",
    "eslint": "5.16.0",
    "eslint-config-loopback": "13.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-ember": "6.6.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "6.1.4",
    "nodemon": "1.19.1",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0"
  }
}
