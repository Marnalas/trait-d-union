{
  "name": "traitdunion",
  "version": "0.0.19",
  "description": "",
  "scripts": {
    "postinstall": "run-p --print-label install:front install:api",
    "install:front": "cd front && npm install",
    "install:api": "cd api && npm install",
    "build": "run-s --print-label build:front cp:front",
    "build:front": "cd front && npm run build",
    "cp:front": "cp -r front/dist/* api/client",
    "test": "run-p --print-label test:front test:api",
    "test:front": "cd front && npm run test",
    "test:api": "cd api && npm run test",
    "test:api:watch": "cd api && npm run test:watch",
    "lint": "run-p --print-label lint:front lint:api",
    "lint:fix": "run-p --print-label lint:front:fix lint:api:fix",
    "lint:front": "cd front && npm run lint",
    "lint:front:fix": "cd front && npm run lint -- --fix",
    "lint:api": "cd api && npm run lint",
    "lint:api:fix": "cd api && npm run lint -- --fix",
    "start": "run-p --print-label start:api",
    "start:api": "cd api && npm run start",
    "start:dev": "cd api && nodemon .",
    "test:e2e:api": "cd api && npm run test:e2e"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/trait-d-union.git"
  },
  "author": "Beta Gouv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/trait-d-union/issues"
  },
  "homepage": "https://github.com/betagouv/trait-d-union#readme",
  "dependencies": {
    "ember-cli": "3.8.2",
    "husky": "1.3.1",
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "devDependencies": {
    "cucumber": "5.1.0",
    "cucumber-pretty": "1.5.0",
    "eslint": "5.16.0",
    "eslint-config-loopback": "13.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-ember": "6.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "mosic": "1.2.0",
    "nodemon": "1.18.11"
  }
}
