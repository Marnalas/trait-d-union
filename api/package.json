{
  "name": "api",
  "version": "0.0.113",
  "private": false,
  "main": "server/server.js",
  "description": "Back end application for Trait d'Union",
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node .",
    "start:dev": "export $(cat .dev.env | xargs) && nodemon .",
    "test:e2e": "export $(cat .e2e.env | xargs) && cucumber-js tests/e2e/** --require tests/e2e/step-definitions/steps.js --format ../node_modules/cucumber-pretty",
    "test": "NODE_ENV=test mocha **/*.test.js --exclude **/node_modules/**",
    "test:watch": "npm run test -- --watch --reporter=min",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-format": "0.2.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "got": "9.6.0",
    "helmet": "3.18.0",
    "joi": "14.3.1",
    "loopback": "3.25.1",
    "loopback-boot": "2.28.0",
    "loopback-component-explorer": "6.4.0",
    "loopback-connector-postgresql": "3.6.1",
    "loopback-ds-calculated-mixin": "2.0.4",
    "loopback-ds-timestamp-mixin": "3.4.1",
    "lru-cache": "5.1.1",
    "luxon": "1.13.2",
    "node-schedule": "1.3.2",
    "node-slack": "0.0.7",
    "request-promise-native": "1.0.7",
    "serve-favicon": "2.5.0",
    "sib-api-v3-sdk": "7.1.4",
    "strong-error-handler": "3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/trait-d-union.git"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "nock": "10.0.6",
    "proxyquire": "2.1.0"
  }
}
