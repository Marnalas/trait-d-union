{
  "name": "ActionFormation",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "action-formations"
  },
  "mixins": {
    "Calculated": {
      "properties": {
        "niveauQualificationEntree": {
          "recalculateOnUpdate": true,
          "callback": "computeNiveauQualificationEntree"
        }
      }
    }
  },
  "properties": {
    "numero": {
      "type": "number",
      "id": true,
      "required": true
    },
    "niveauQualificationSortie": {
      "type": "string"
    },
    "niveauQualificationEntree": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "sessions": {
      "type": "hasMany",
      "model": "SessionFormation",
      "foreignKey": "actionNumero"
    },
    "diplome": {
      "type": "belongsTo",
      "model": "Diplome",
      "foreignKey": "codeDiplome"
    }
  },
  "acls": [],
  "methods": {}
}
