{
  "name": "Offre",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "offres"
  },
  "scope": {
    "order": "createdAt DESC"
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    },
    "SetupRemoteMethods": {
      "disableAllExcept": [
        "find",
        "findById",
        "sourceOffres"
      ]
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "required": true
    },
    "data": {
      "type": "object"
    },
    "status": {
      "type": "string",
      "required": false,
      "default": "available"
    }
  },
  "validations": [],
  "relations": {
    "candidatures": {
      "type": "hasMany",
      "model": "Candidat",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      },
      "through": "Candidature"
    }
  },
  "acls": [],
  "methods": {}
}
