{
  "name": "SessionFormation",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "session-formations"
  },
  "properties": {
    "numero": {
      "type": "number",
      "required": true
    },
    "dateDebut": {
      "type": "date",
      "required": true
    },
    "dateFin": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "action": {
      "type": "belongsTo",
      "model": "ActionFormation",
      "foreignKey": "actionNumero"
    },
    "commune": {
      "type": "belongsTo",
      "model": "Commune",
      "foreignKey": "communeCodeInsee"
    }
  },
  "acls": [],
  "methods": {}
}
