{
  "name": "SessionFormation",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "session-formations"
  },
  "properties": {
    "numero": {
      "type": "number",
      "required": true
    },
    "dateDebut": {
      "type": "date",
      "required": false
    },
    "dateFin": {
      "type": "date",
      "required": false
    },
    "duration": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "action": {
      "type": "belongsTo",
      "model": "ActionFormation",
      "foreignKey": "actionNumero"
    },
    "commune": {
      "type": "belongsTo",
      "model": "Commune",
      "foreignKey": "communeCodeInsee"
    }
  },
  "acls": [],
  "methods": {},
  "mixins": {
    "Calculated": {
      "properties": {
        "duration": {
          "recalculateOnUpdate": true,
          "callback": "computeDuration"
        }
      }
    }
  }
}
