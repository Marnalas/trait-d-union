{
  "name": "Candidat",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "candidats"
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    },
    "SetupRemoteMethods": {
      "disableAllExcept": [
        "create",
        "find",
        "findById",
        "formResponse",
        "prototype.__link__candidatures"
      ]
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "required": true,
      "defaultFn": "uuidv4"
    },
    "email": {
      "type": "string",
      "id": true,
      "required": true
    },
    "nomPrenom": {
      "type": "string",
      "required": true
    },
    "telephone": {
      "type": "string",
      "id": true,
      "required": true
    },
    "cvUrl": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "candidatures": {
      "type": "hasMany",
      "model": "Offre",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      },
      "through": "Candidature"
    }
  },
  "acls": [],
  "methods": {}
}
